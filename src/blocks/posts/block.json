{
  "name": "advanced-posts-blocks/posts",
  "apiVersion": 2,
  "title": "Multiple Posts",
  "description": "Display multiple posts.",
  "icon": "admin-post",
  "textdomain": "advanced-posts-blocks",
  "attributes": {
    "postType": {
      "type": "string",
      "default": "post"
    },
    "postsToShow": {
      "type": "number",
      "default": 5
    },
    "offset": {
      "type": "number",
      "default": 0
    },
    "order": {
      "type": "string",
      "default": "desc"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "ignoreStickyPosts": {
      "type": "boolean",
      "default": true
    },
    "showAllPosts": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "category": "widgets",
  "editorScript": "file:../../../build/blocks/posts/index.js"
}
