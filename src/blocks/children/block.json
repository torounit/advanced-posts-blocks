{
	"name": "advanced-posts-blocks/children",
	"apiVersion": 2,
	"title": "Child Posts",
	"description": "Display children of selected post.",
	"icon": "admin-page",
	"textdomain": "advanced-posts-blocks",
	"attributes": {
		"align": {
			"type": "string",
			"enum": [
				"center",
				"wide",
				"full"
			]
		},
		"postType": {
			"type": "string",
			"default": "page"
		},
		"postId": {
			"type": "number",
			"default": 0
		},
		"postsToShow": {
			"type": "number",
			"default": -1
		},
		"order": {
			"type": "string",
			"default": "asc"
		},
		"orderBy": {
			"type": "string",
			"default": "title"
		}
	},
	"supports": {
		"align": [
			"center",
			"wide",
			"full"
		],
		"html": false
	},
	"category": "widgets",
	"editorScript": "file:./index.js"
}
